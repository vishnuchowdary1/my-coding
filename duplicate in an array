import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		int c=0,temp=0,k=-1;
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
{
    a[i]=sc.nextInt();
}
Arrays.sort(a);
for(int i=0;i<n;i++)
{
 for(int j=i+1;j<n;j++)
 {
     if(a[i]==a[j])
     { 
         a[j]=k;
         k--;
       c++;   
     }
 }
 if(c>=1)
 {
     temp++;
     c=0;
 }
}
System.out.print(temp);

	}
}
