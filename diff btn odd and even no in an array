import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		int sum1=0,sum2=0;
Scanner sc=new Scanner(System.in);
int b=sc.nextInt();
int a[]=new int[25];
for(int i=0;i<b;i++)
{
    a[i]=sc.nextInt();

if((a[i]%2)==0)
{
    sum1=sum1+a[i];
}
else{
    sum2=sum2+a[i];
}
}
System.out.println(sum2-sum1);
	}
}
