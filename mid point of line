import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
 int a,b,c,d;
 float e,f;
 Scanner sc=new Scanner(System.in);
 a=sc.nextInt();
 b=sc.nextInt();
 c=sc.nextInt();
 d=sc.nextInt();
 e=((float)(a+c))/2.0f;
 f=((float)(b+d))/2.0f;
 System.out.print(e+"\n"+f);

	}
}
