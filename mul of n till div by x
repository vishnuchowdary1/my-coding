import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int x=sc.nextInt();
int add=n;
for(int i=0;i>=0;i++){
if(i!=0){
    n=n+add;
}
if((n%x)!=0)
{
    System.out.println(n+" ");
}
else
{
    System.out.println(n);
    break;
}
}
	}
}
